doctype html
html
	head(lang="en")
		meta(charset="UTF-8")
		title Rory Shively - Design &amp; Development
		meta(name="viewport", content="width=device-width, initial-scale=1.0, user-scalable=no")
		meta(name="author", content="Rory Shively")
		link(rel="stylesheet", href="css/main.css", type="text/css")
		link(rel="stylesheet", href="css/onepage-scroll.css", type="text/css")
		link(rel="stylesheet", href="http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic", type="text/css")
		link(rel="stylesheet", href="http://fonts.googleapis.com/css?family=Pirata+One")
		script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
		script(src="js/all.min.js")
		script(src="js/nav.js")
		script(src="js/jquery.onepage-scroll.js")
		
	body.cbp-spmenu-push
		nav.cbp-spmenu.cbp-spmenu-vertical.cbp-spmenu-left(id="cbp-spmenu-s1")
			h3 Menu
			a(href="#") Watch Pilot
			a(href="#") Behind the Scenes
			a(href="#") Cast &amp; Crew
		.main.onepage-wrapper
			section.section.1(style="background:url(img/aw2.jpg);background-size:cover;background-position:center;")
				button(id="showLeftPush") Menu
				.logo
					img(src="img/aw.svg", data-selector="logo")
					ul
						li
							a(href="#", data-selector="li1") Watch Pilot
						li
							a(href="#", data-selector="li2") Behind the Scenes
						li
							a(href="#", data-selector="li3") Cast &amp; Crew
			section.section.2(style="background-color:tan;")
				button(id="showLeftPush2") Menu
			section.section.3(style="background-color:lightblue;")
				button(id="showLeftPush3") Menu
			section.section.4(style="background-color:lavender;")
				button(id="showLeftPush4") Menu
			section.section.5(style="background-color:#111;")
				button(id="showLeftPush5") Menu
		script(src="js/call.js")